{
  "name": "file-search-query",
  "version": "0.2.0",
  "contextFileName": "GEMINI.file-search.md",
  "mcpServers": {
    "file-search": {
      "command": "${extensionPath}${/}file-search",
      "args": [
        "mcp"
      ],
      "cwd": "${workspacePath}"
    }
  },
  "settings": [
    {
      "name": "Gemini API Key",
      "description": "Your Google Gemini API Key",
      "envVar": "GEMINI_API_KEY",
      "sensitive": true
    }
  ]
}